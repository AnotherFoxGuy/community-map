material BM-hghschl_window
{
	receive_shadows on
	technique
	{
		pass
		{
			ambient 0.500000 0.500000 0.500000 0.960000
			diffuse 0.294902 0.294902 0.294902 0.960000
			specular 0.000000 0.000000 0.000000 0.960000 12.500000
			emissive 0.000000 0.000000 0.000000 0.960000
			scene_blend alpha_blend
			depth_write off
			texture_unit
			{
				texture window.dds
				tex_address_mode wrap
				filtering trilinear
				colour_op alpha_blend
			}
			texture_unit
			{
				texture light.dds
				colour_op alpha_blend
			}
            texture_unit 
            {
                texture highschool-AO.dds
                tex_address_mode wrap
                scale 1.0 1.0
                tex_coord_set 1
                colour_op modulate
            }
        }
    }
}
